00  READ  09    ;   (Ввод A)
01  READ  10    ;   (Ввод B)
02  LOAD  09    ;   (Загрузка A в аккумулятор)
03  SUB   10    ;   (Отнять B)
04  JNEG  07    ;   (Переход на 07, если отрицательное)
05  WRITE 09    ;   (Вывод A)
06  HALT  00    ;   (Останов)
07  WRITE 10    ;   (Вывод B)
08  HALT  00    ;   (Останов)
09  =     +0002 ;   (Переменная A)
10  =     -0004 ;   (Переменная B)